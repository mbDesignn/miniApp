.prd-info {
  margin-top:53rpx;
  display: flex;
  justify-content: space-between;
  flex-direction: column;
  text-align: center;
  align-items: cneter;
}
..proinfo-ps text{
  word-break:keep-all;
  white-space: nowrap;
}
.prd-info > text:nth-of-type(1),.prd-info > rich-text:nth-of-type(1) {
  font-size: 40rpx;
  color: #003b6f;
  font-weight: normal;
}
.prd-info > rich-text:nth-of-type(1) {
  margin-top:-20rpx;
}

.prd-info > text:nth-of-type(2) {
  font-size: 19rpx;
  color: #33a9c1;
}

.prd-info > text:nth-of-type(3) {
  padding-top: 6rpx;
  font-size: 12rpx;
  color: #999;
}

.prd-info > button {
  width: 204rpx;
  height: 50rpx;
  font-size: 26rpx;
  line-height: 50rpx;
  background: #003b6f;
  border-radius: 0;
  color: #fff;
}

.prd-info > button::after {
  border: 0;
}

.prd-info > view {
  margin-bottom: -12rpx;
}

.prd-info > view > text {
  color: #003b6f;
}

.prd-info > view > text:nth-of-type(1) {
  font-size: 20rpx;
}

.prd-info > view > text:nth-of-type(2) {
  font-size: 60rpx;
}

.prd-info > view > text:nth-of-type(3) {
  font-size: 20rpx;
}

.prd-pt {
  padding-top: 0rpx;
}

