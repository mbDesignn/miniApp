.book-wrap{
  background: #f3f3f3;
  height: 100vh;
}
.add-address{
  padding-left:32rpx;
	height: 143rpx;
	background-color: #ffffff;
  display: flex;
  align-items: center;
}
.add-address > text{
  margin-left: 15rpx;
}
.add-address > text:nth-of-type(2){
  color: #dc1640;
}

.product {
  margin:20rpx;
  display: flex;
  justify-content: start;
  align-items: center;
	height: 267rpx;
	background-color: #ffffff;
	border-radius: 10rpx;
}
.product-select{
  width:142rpx;
  text-align: center;
}
.product-img{
  width:164rpx;
}
.product-img image{
  width:164rpx;
  height: 177rpx;
}
.product-info{
  position: relative;
  padding-right: 60rpx;
  padding-left:27rpx;
  /* display: flex; */
  /* align-items: flex-start; */
  width:560rpx;
}
.product-info text{
  display: block;
}
.product-title {
	font-family: SourceHanSansCN-Regular;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 36rpx;
	letter-spacing: 0rpx;
	color: #323232;
}
.product-title-m {
	font-family: SourceHanSansCN-Regular;
	font-size: 18rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 36rpx;
	letter-spacing: 0rpx;
	color: #9e9e9e;
}
.product-price {
  padding-top:42rpx;
	font-family: SourceHanSansCN-Medium;
	font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 36rpx;
	letter-spacing: 0rpx;
	color: #dc1640;
}
.count-wrap > view{
  padding: 0 32rpx;
  display: flex;
  justify-content: space-between;
  align-items:center;
  min-height: 100rpx;
  background: #fff;
  border-bottom:1px solid #e3e3e3;
}
.count-wrap > view:last-of-type{
  border-bottom:0rpx;
}
.right-txt{
  color: #dc1640;
}

.active-wrap{
  padding-top:45rpx;
  padding-bottom:45rpx;
}
.active-bar{
  padding-top:26rpx;
  display: flex;
}
.active-t{
	width: 118rpx;
	height: 28rpx;
	font-family: SourceHanSansCN-Regular;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 36rpx;
	letter-spacing: 0rpx;
	color: #1d1d1d;
}
.active-bar input{
  margin-right:20rpx;
  padding-left:20rpx;
  width: 490rpx;
	height: 72rpx;
	background-color: #ffffff;
	border-radius: 8rpx;
	border: solid 2rpx #cccccc;
}
.active-bar button{
  width: 154rpx;
	height: 72rpx;
  line-height:72rpx;
	background-color: #000000;
	border-radius: 8rpx;
  color:#fff;
}